# ---- Security / Auth ----
JWT_SECRET=<generate_new_random_64+chars>
JWT_AUD=northstar
JWT_ISS=northstar
TENANT_ID=tenant1

# ---- Postgres (RDS) ----
POSTGRES_HOST=<rds-endpoint>.rds.amazonaws.com
POSTGRES_PORT=5432
POSTGRES_DB=<db_name>
POSTGRES_USER=<app_user>
# If you ever `source` this file in a shell, wrap in single quotes or escape $ as \$
POSTGRES_PASSWORD='<strong_random_password>'
POSTGRES_SSLMODE=require
POSTGRES_CONNECT_TIMEOUT=10

# ---- Qdrant Cloud ----
QDRANT_URL=https://<your-cluster>.aws.cloud.qdrant.io:6333
QDRANT_API_KEY=<rotate_new_qdrant_api_key>
QDRANT_COLLECTION_PREFIX=tenant_
HYBRID_STRATEGY_DEFAULT=qdrant
HYBRID_TOPK=12
HYBRID_LAMBDA=0.6

# ---- Embeddings / Retrieval ----
EMBEDDING_MODEL=bge-m3

# ---- Queue (Upstash Redis) ----
QUEUE_MODE=redis
REDIS_URL=rediss://default:<rotate_new_upstash_token>@<your-db>.upstash.io:6379
QUEUE_NAMESPACE=northstar
RATE_LIMITER_ENABLED=true

# ---- Router / Policy Flags ----
ROUTER_MODE=static
POLICY_ENFORCEMENT=strict
POLICY_DENY_PROJECTS=
POLICY_PROOF_MODE=false
ISOLATION_REPORT_DIR=./artifacts/isolation
EVAL_DATA_DIR=./eval/data
EVAL_MODE=false
ABMAP_ENABLED=false

# ---- Cerebras (Planner/Agent) ----
LLM_PROVIDER=cerebras
CEREBRAS_API_KEY=<rotate_new_cerebras_key>
CEREBRAS_BASE_URL=https://api.cerebras.ai
CEREBRAS_MODEL=<your_model_id>
CEREBRAS_TIMEOUT_SEC=60
CEREBRAS_MAX_RETRIES=2

# ---- Atlassian ----
ATLASSIAN_BASE_URL=https://<your-domain>.atlassian.net
ATLASSIAN_EMAIL=<your_email>
ATLASSIAN_API_TOKEN=<rotate_new_atlassian_token>
ATLASSIAN_PROJECT_KEY=PX
ATLASSIAN_CONFLUENCE_SPACE=ENG

# ---- GitHub webhook ----
GITHUB_WEBHOOK_SECRET=<YOUR_GITHUB_WEBHOOK_SECRET>
